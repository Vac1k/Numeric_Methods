"print(f'R(f) = {round(M2 * np.abs(a - b) * h ** 2 / 12, int(abs(np.log10(e))) + 2)}')\n","\n","print(f'n = {int(n)}')\n","int_tr = trapeze_method(f, a, b, n)\n","print(f'Значення інтегралу методом трапецій {int_tr}')\n","\n","print('\\nМетод Сімпсона\\n')\n","\n","n_for_simpson = 10\n","ih=simpson_method(f, a, b, n_for_simpson)\n","n_for_simpson = n_for_simpson*2\n","ih2=simpson_method(f, a, b, n_for_simpson)\n","#Правило Рунге\n","while abs(ih-ih2) / 15 > 1e-5:\n","    n_for_simpson = n_for_simpson*2\n","    ih=ih2\n","    ih2=simpson_method(f, a, b, n_for_simpson)\n","print(f'n = {n_for_simpson}')\n","int_simp=ih2\n","print(f'Значення інтегралу методом Сімпсона {int_simp}')\n"],"metadata":{"id":"4G0FftSOcpC0","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1703716849962,"user_tz":-120,"elapsed":1654,"user":{"displayName":"Maksym Shkarupylo","userId":"07461948744333407479"}},"outputId":"174fb324-6022-43eb-e369-1a77277d592f"},"execution_count":14,"outputs":[{"output_type":"stream","name":"stdout","text":["Метод Трапецій\n","\n","M2 = 0.434333\n","h = 0.0133\n","R(f) = 1e-05\n","n = 119\n","Значення інтегралу методом трапецій 0.19463207968417304\n","\n","Метод Сімпсона\n","\n","n = 20\n","Значення інтегралу методом Сімпсона 0.1946289038492277\n"]}]},{"cell_type":"code","source":[],"metadata":{"id":"lnwwZK5sjvTX"},"execution_count":null,"outputs":[]}]}